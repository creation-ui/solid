{
  "name": "@creation-ui/solid",
  "version": "0.1.0",
  "description": "Solid.js UI library.",
  "author": {
    "name": "PaweÅ‚ Krystkiewicz",
    "email": "pawelkrystkiewicz@outlook.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "solid-start dev --open",
    "build": "solid-start build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "nuke": "rm -rf node_modules yarn.lock pnpm-lock.yaml && pnpm install",
    "sitemap": "",
    "package:version": "pnpm --filter ./packages/solid ci:version",
    "package:build": "pnpm rm:build && pnpm --filter ./packages/solid build",
    "package:publish": "pnpm --filter ./packages/solid _publish",
    "package": "pnpm package:version && pnpm package:build && pnpm package:publish",
    "export": "next export",
    "ci:publish": "pnpm publish -r",
    "ci:version": "git fetch --tags && npm version $(git describe --abbrev=0 --tags)",
    "amend": "git commit --amend --no-edit",
    "import-check": "npx cycle-import-check",
    "rm:build": "rm -rf ./packages/solid/build",
    "e2e": "pnpm package:build && cypress run",
    "e2e:dev": "pnpm package:build && cypress open",
    "override-version": "sh ./override-version.sh",
    "generate:docs": "node ./scripts/generate-docs.js",
    "prettify": "prettier --write .",
    "core": "pnpm -r add @creation-ui/core@latest"
  },
  "type": "module",
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/node": "^20.6.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "solid-devtools": "^0.27.7",
    "solid-start-node": "^0.3.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@creation-ui/core": "^0.4.1",
    "@mdi/js": "^7.2.96",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash-es": "^4.17.9",
    "@types/prismjs": "^1.26.0",
    "babel-plugin-prismjs": "^2.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "git-url-parse": "^13.1.0",
    "js": "link:@types/@mdi/js",
    "lodash-es": "^4.17.21",
    "lodash.get": "^4.4.2",
    "prismjs": "^1.29.0",
    "react-code-blocks": "^0.1.4",
    "solid-js": "^1.7.11",
    "solid-start": "^0.3.5",
    "tailwind-merge": "^1.14.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
