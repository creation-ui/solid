{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components/"],
      "@creation-ui/core/*": ["../creation-ui-react/packages/core/src/*"],
      "@creation-ui/core": ["../creation-ui-react/packages/core/src"],
      "@creation-ui/solid/*": ["./packages/solid/src/*"],
      "@creation-ui/solid": ["./packages/solid/src"],
      "@examples/*": ["./src/components/examples/*"],
      "@examples": ["./src/components/examples/"],
      "@routes/*": ["routes/*"],
      "@routes": ["routes"],
      "@utils/*": ["utils/*"],
      "@utils": ["utils"],
      "@models/*": ["./models/*"],
      "@models": ["./models"],
      "@root/*": ["./*"],
      "@root": ["./"]
    },
  }
}
