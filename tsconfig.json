{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@playground/*": ["./src/playground/*"],
      "@playground": ["./src/playground/"],
      "@docs/*": ["./src/docs/*"],
      "@docs": ["./src/docs/"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components/"],
      "@creation-ui/solid/*": ["./packages/solid/src/*"],
      "@creation-ui/solid": ["./packages/solid/src"],
      "@examples/*": ["./src/components/examples/*"],
      "@examples": ["./src/components/examples/"],
      "@routes/*": ["routes/*"],
      "@routes": ["routes"],
      "@utils/*": ["utils/*"],
      "@utils": ["utils"],
      "@models/*": ["./models/*"],
      "@models": ["./models"],
      "@root/*": ["./*"],
      "@root": ["./"],
      "@src/*": ["./src/*"],
      "@src": ["./src"]
    }
  }
}
